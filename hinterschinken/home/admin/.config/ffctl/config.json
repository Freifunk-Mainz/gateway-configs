{
  "basedir": "/home/admin/clones/ffctl",
  "ecdsakey": "/home/admin/.ecdsa/hinterschinken.prv",
  "ffctl_ident": "ffctl",
  "ffctl_version": "25e264d",
  "githubident": "github_ffctl",
  "hostname": "hinterschinken",
  "io_encoding": "utf-8",
  "loglen": 1048576,
  "loglevel": 20,
  "logskeep": 23,
  "mailalert": true,
  "mailprefix": "freifunk-mainz.de",
  "mailrcpt": "admin@freifunk-mainz.de",
  "meta": {
    "configb_copy": "copied backup of config",
    "git_mergeconflict": "merge conflicts",
    "git_publish": "published all changes",
    "git_save": "synchronized all changes",
    "git_setup": "prepared new git repo",
    "import_json": "json import",
    "import_meta": "meta import",
    "stage_new": "opened new stage. please applaud",
    "stage_switch_in": "switched into stage",
    "stage_switch_out": "switched out of stage"
  },
  "mod": {
    "confb_queuefile": "queue.json",
    "confb_remote": "ssh://github_ffctl/Freifunk-Mainz/gateway-configs.git",
    "confb_subfolder": "gateway-configs.git"
  },
  "pycmd": "python3",
  "reg": {
    "mz": {
      "bat": {
        "gw": "64MBit/64MBit",
        "if": "mzBAT"
      },
      "exit": {
        "if": "exitVPN"
      },
      "fastd": {
        "if": "mzVPN",
        "peers": "/etc/fastd/mzVPN/peers",
        "remote": "ssh://github_ffctl/Freifunk-Mainz/peers-ffmz.git"
      }
    },
    "wi": {
      "bat": {
        "gw": "64MBit/64MBit",
        "if": "wiBAT"
      },
      "exit": {
        "if": "exitVPN"
      },
      "fastd": {
        "if": "wiVPN",
        "peers": "/etc/fastd/wiVPN/peers",
        "remote": "ssh://github_ffctl/Freifunk-Mainz/peers-ffwi.git"
      }
    }
  }
}